<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.group6.sjsulookout">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <activity
            android:name=".LoginActivity"
            android:label="SJSULookout">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity
            android:name=".SignUpActivity"
            android:label="Sign Up" />
        <activity
            android:name=".AllEvents"
            android:label="Discover Events" />
        <activity
            android:name=".AddEvent"
            android:label="Add Event" />
        <activity
            android:name=".UserEvents"
            android:label="User Events" />
        <activity
            android:name=".EventPage"
            android:label="Event"></activity>
        <activity
            android:name=".AttendingEvents"
            android:label="Attending Events"></activity>
    </application>

</manifest>